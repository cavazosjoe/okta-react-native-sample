{
  "name": "SecureApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@okta/okta-react-native": "2.15.0",
    "events": "3.3.0",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@react-native/gradle-plugin": "0.76.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/cli": "latest",
    "@react-native/babel-preset": "0.76.5",
    "babel-plugin-react-native-web": "0.19.13",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "babel-jest": "^29.7.0",
    "babel-plugin-syntax-hermes-parser": "^0.26.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.8",
    "enzyme-async-helpers": "0.9.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^26.8.7",
    "hermes-parser": "0.26.0",
    "jest": "^28.1.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.8.8",
    "react-dom": "18.3.1",
    "react-native-codegen": "^0.72.0",
    "react-test-renderer": "18.3.1"
  },
  "jest": {
    "preset": "react-native",
    "automock": false,
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "node_modules/(?!@okta|@react-native|react-native)"
    ],
    "testMatch": [
      "**/tests/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.(js)$": ["babel-jest", { "plugins": ["babel-plugin-syntax-hermes-parser"] }],
      "^.+\\.(ts|tsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
